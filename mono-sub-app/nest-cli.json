{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/mono-sub-app/src",
  "monorepo": true,
  "root": "apps/mono-sub-app",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/mono-sub-app/tsconfig.app.json"
  },
  "projects": {
    "mono-sub-app": {
      "type": "application",
      "root": "apps/mono-sub-app",
      "entryFile": "main",
      "sourceRoot": "apps/mono-sub-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/mono-sub-app/tsconfig.app.json"
      }
    },
    "cat": {
      "type": "application",
      "root": "apps/cat",
      "entryFile": "main",
      "sourceRoot": "apps/cat/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cat/tsconfig.app.json"
      }
    }
  }
}